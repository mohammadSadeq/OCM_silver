<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="HomePageRegression" parallel="tests" thread-count="10" data-provider-thread-count="2">
